// Place your settings in this file to overwrite the default settings
{
    // Editor
    "editor.tabSize": 2,
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "all",
    "window.zoomLevel": 1,
    "editor.suggestOnTriggerCharacters": false,
    "typescript.check.tscVersion": false,
    "workbench.colorTheme": "Monokai",
    "editor.minimap.enabled": true,
    "files.exclude": {
      "**/*.js": { "when": "$(basename).ts" },
      "**/*.map": { "when": "$(basename).map" },
      "**/*.css": { "when": "$(basename).scss" }
    },
    "typescript.check.npmIsInstalled": false,
    "workbench.startupEditor": "newUntitledFile",
    "gitlens.blame.line.enabled": false,
    "prettier.trailingComma": "all",
    "prettier.singleQuote": true,
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 13,
    "extensions.ignoreRecommendations": false,
    "jupyter.appendResults": false,
    "gitlens.advanced.messages": {
      "suppressCommitHasNoPreviousCommitWarning": false,
      "suppressCommitNotFoundWarning": false,
      "suppressFileNotUnderSourceControlWarning": false,
      "suppressGitVersionWarning": false,
      "suppressLineUncommittedWarning": false,
      "suppressNoRepositoryWarning": false,
      "suppressResultsExplorerNotice": false,
      "suppressShowKeyBindingsNotice": true,
      "suppressUpdateNotice": true,
      "suppressWelcomeNotice": false
    },
    "vim.cmdLineInitialColon": true,
    "vim.useSystemClipboard": true,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.keymap": "chorded",
    "bracketPairColorizer.timeOut": 0,
    "gitlens.historyExplorer.enabled": true,
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.enableNeovim": false,
    "vim.statusBarColorControl": true,
    "vim.easymotionMarkerFontFamily": "Fira Code",
    "vim.easymotionMarkerYOffset": 5,
    "editor.lineNumbers": "on",
    "vim.statusBarColors": {
        "normal": ["#30302c", "#8197bf"],
        "insert": ["#30302c", "#99ad6a"],
        "visual": ["#30302c", "#f0a0c0"],
        "visualline": ["#30302c", "#f0a0c0"],
        "visualblock": ["#30302c", "#f0a0c0"],
        "replace": ["#30302c", "#cf6a4c"],
    },
    "workbench.colorCustomizations": {
      "statusBar.background": "#30302c",
      "statusBar.noFolderBackground": "#30302c",
      "statusBar.debuggingBackground": "#30302c",
      "statusBar.foreground": "#8197bf"
    },
    "reason.codelens.enabled": true,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "peacock.favoriteColors": [
      {
        "name": "Angular Red",
        "value": "#b52e31"
      },
      {
        "name": "Auth0 Orange",
        "value": "#eb5424"
      },
      {
        "name": "Azure Blue",
        "value": "#007fff"
      },
      {
        "name": "C# Purple",
        "value": "#68217A"
      },
      {
        "name": "Gatsby Purple",
        "value": "#639"
      },
      {
        "name": "Go Cyan",
        "value": "#5dc9e2"
      },
      {
        "name": "Java Blue-Gray",
        "value": "#557c9b"
      },
      {
        "name": "JavaScript Yellow",
        "value": "#f9e64f"
      },
      {
        "name": "Mandalorian Blue",
        "value": "#1857a4"
      },
      {
        "name": "Node Green",
        "value": "#215732"
      },
      {
        "name": "React Blue",
        "value": "#00b3e6"
      },
      {
        "name": "Something Different",
        "value": "#832561"
      },
      {
        "name": "Vue Green",
        "value": "#42b883"
      }
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.defaultDateStyle": "absolute",
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
  }